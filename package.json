{
  "name": "video-capture",
  "version": "1.0.0",
  "description": "A chrome extension for capture video element with Gyazo.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ssig33/video-capture.git"
  },
  "scripts": {
    "fetch-and-build": "npm run-script fetch && npm run-script build",
