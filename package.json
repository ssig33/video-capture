    "file-loader": "^5.0.2",
    "fs-extra": "^8.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "^4.0.9",
    "lint-staged": "^9.5.0",
    "prettier": "1.19.1",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "write-file-webpack-plugin": "^4.5.1"
